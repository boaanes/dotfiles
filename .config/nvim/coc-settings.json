{
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        ".stack.yaml",
        ".hie-bios",
        "BUILD.bazel",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ]
    }
  },
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
      "css",
      "markdown",
      "typescriptreact",
      "javascriptreact",
      "typescript",
      "javascript"
  ]
}
